
@using Experts.First_Project.Web.Areas.App.Models.Common.Modals
@using Experts.First_Project.Web.Areas.App.Models.Governatates
@model GovernatateViewModel

@await Html.PartialAsync("~/Areas/App/Views/Common/Modals/_ModalHeader.cshtml", new ModalHeaderViewModel(L("Details")))

<div class="modal-body">
    <div id="GovernatateInformationsTab">

        <div class="row">
            <div class="col-6 mb-3">
                <h6>@L("code") :</h6>
                @Model.Governatate.code
            </div>
            <div class="col-6 mb-3">
                <h6>@L("Name") :</h6>
                @Model.Governatate.Name
            </div>

        </div>

    </div>
</div>

@await Html.PartialAsync("~/Areas/App/Views/Common/Modals/_ModalFooterWithClose.cshtml")